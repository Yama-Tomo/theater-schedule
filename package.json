{
  "name": "theater-schedule",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx,.js,.vue --format codeframe --color",
    "dev": "nuxt-ts",
    "gen": "nuxt-ts generate",
    "build": "nuxt-ts build && tsc -p tsconfig.nuxt-config.json",
    "start": "nuxt start -c .nuxt/ts-compiled/nuxt.config.js",
    "build:netlify": "wget -O - $SCHEDULES_DATA_ENDPOINT > src/data/schedules.json && yarn gen"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/sitemap": "2.4.0",
    "bulma": "0.9.1",
    "nuxt": "2.14.9",
    "nuxt-property-decorator": "2.8.8",
    "vuex-type-helper": "1.2.2"
  },
  "devDependencies": {
    "@nuxt/types": "2.14.9",
    "@nuxt/typescript-build": "2.0.3",
    "@nuxt/typescript-runtime": "2.0.0",
    "@nuxtjs/eslint-config-typescript": "5.0.0",
    "@types/hard-source-webpack-plugin": "1.0.1",
    "@types/lodash": "4.14.165",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-prettier": "3.2.0",
    "hard-source-webpack-plugin": "0.13.1",
    "husky": "4.3.0",
    "json-loader": "0.5.7",
    "lint-staged": "10.5.3",
    "node-sass": "5.0.0",
    "pug": "3.0.0",
    "pug-plain-loader": "1.0.0",
    "sass-loader": "10.1.0",
    "ts-loader": "7.0.5",
    "ts-node": "9.1.0",
    "typescript": "4.1.2",
    "yaml-loader": "0.6.0"
  }
}

{
  "name": "theater-schedule",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint --no-fix || true",
    "lint:commit": ".scripts/lint_when_precommit",
    "dev": "nuxt-ts",
    "gen": "nuxt-ts generate",
    "build": "nuxt-ts build && tsc -p tsconfig.nuxt-config.json",
    "start": "nuxt start -c .nuxt/ts-compiled/nuxt.config.js",
    "build:netlify": "wget -O - $SCHEDULES_DATA_ENDPOINT > src/data/schedules.json && NODE_ENV=production npm run gen"
  },
  "pre-commit": [
    "lint:commit"
  ],
  "dependencies": {
    "@nuxtjs/google-analytics": "2.2.0",
    "@nuxtjs/sitemap": "0.2.2",
    "@types/node": "10.14.4",
    "bulma": "0.7.4",
    "nuxt": "2.4.5",
    "nuxt-property-decorator": "2.1.3",
    "vuex-type-helper": "1.2.1"
  },
  "devDependencies": {
    "@types/hard-source-webpack-plugin": "1.0.0",
    "@types/webpack": "4.4.26",
    "@vue/cli-plugin-babel": "3.5.3",
    "@vue/cli-plugin-typescript": "3.5.3",
    "@vue/cli-service": "3.5.3",
    "hard-source-webpack-plugin": "0.13.1",
    "json-loader": "0.5.7",
    "node-sass": "4.11.0",
    "nuxt-ts": "2.4.5",
    "pre-commit": "1.2.2",
    "pug": "2.0.3",
    "pug-plain-loader": "1.0.0",
    "sass-loader": "7.1.0",
    "typescript": "3.3.4000",
    "yaml-loader": "0.5.0"
  }
}

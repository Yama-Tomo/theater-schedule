{
  "name": "theater-schedule",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx,.js,.vue --format codeframe --color",
    "dev": "nuxt",
    "gen": "nuxt generate",
    "build": "nuxt build && tsc -p tsconfig.nuxt-config.json",
    "start": "nuxt start -c .nuxt/ts-compiled/nuxt.config.js",
    "build:netlify": "wget -O - $SCHEDULES_DATA_ENDPOINT > src/data/schedules.json && yarn gen",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/sitemap": "2.4.0",
    "bulma": "0.9.3",
    "nuxt": "2.15.8",
    "nuxt-property-decorator": "2.9.1",
    "vuex-type-helper": "1.3.1"
  },
  "devDependencies": {
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@types/hard-source-webpack-plugin": "1.0.3",
    "@types/lodash": "4.14.172",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-prettier": "3.4.1",
    "hard-source-webpack-plugin": "0.13.1",
    "husky": "7.0.1",
    "json-loader": "0.5.7",
    "lint-staged": "11.1.2",
    "node-sass": "6.0.1",
    "prettier": "2.3.2",
    "pug": "3.0.2",
    "pug-plain-loader": "1.1.0",
    "sass-loader": "10.2.0",
    "typescript": "4.3.5",
    "yaml-loader": "0.6.0"
  },
  "resolutions": {
    "typescript": "4.3.5"
  }
}
